{
    "compilerOptions": {
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "strictNullChecks": true,
        "jsx": "react",
        "sourceMap": true,
        "allowJs": true,
        "downlevelIteration": true,
        "importHelpers": true,
    },
    "exclude": [
        "node_modules"
    ]
}